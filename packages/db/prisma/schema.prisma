generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


enum OrderType {
  long
  short
}


model ExistingTrades {

  id         String @id @default(uuid())
  openPrice  Int 
  leverage   Int
  asset      Int 
  quantity   Float
  type       OrderType
  closePrice Int
  pnl        Int
  liquidated Boolean
  createdAt  DateTime @default(now()) 
  
}